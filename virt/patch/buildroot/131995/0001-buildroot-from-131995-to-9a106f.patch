+			#clock-cells = <0>;
+		};
+
+		swrc0: swrc0@1fbd0038 {
+			compatible = "csky,reset-v1";
+			reg = <0x1fbd0038 4>;
+			#reset-cells = <1>;
+		};
+
+		swrc1: swrc1@1fbd003c {
