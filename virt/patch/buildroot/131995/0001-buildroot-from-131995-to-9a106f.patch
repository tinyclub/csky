+++ b/board/qemu/csky/readme.txt
@@ -0,0 +1,26 @@
+C-SKY Development Kit
+
+Intro
+=====
+
+C-SKY QEMU, how to build and run.
+
+For ck810
+=========
+
+  $ make qemu_csky_ck810_uclibc_defconfig
+  $ make
+
+  $ cd output/images
+  $ ../host/csky-qemu/bin/qemu-system-cskyv2 -machine virt -kernel vmlinux -dtb ck810.dtb -nographic
+
+For ck610
+=========
+
+  $ make qemu_csky_ck610_uclibc_defconfig
+  $ make
+
+  $ cd output/images
+  $ ../host/csky-qemu/bin/qemu-system-cskyv1 -machine virt -kernel vmlinux -dtb ck610.dtb -nographic
+
+
diff --git a/board/silan/sc6138a/gdbinit b/board/silan/sc6138a/gdbinit
new file mode 100644
index 0000000..5931042
--- /dev/null
+++ b/board/silan/sc6138a/gdbinit
@@ -0,0 +1,20 @@
+target jtag jtag://127.0.0.1:1025
+#file reset
+#lo
+#c
+file u-boot
+lo
+c
+set $cr18=0x7d
+
+# pass devicetree blob
+# r2 is magic
+# r3 is address
+set $r2 = 0x20150401
+set $r3 = 0x81000000
+restore sc6138a.dtb binary 0x81000000
+
+
+file vmlinux  
+lo
+
