+			gpio0: gpio0@1fbac000 {
+				compatible = "csky,gpio-bank-v1";
+				reg = <0x1fbac000 0x1000>;
+
+				gpio-controller;
+				#gpio-cells = <2>;
+
+				interrupt-controller;
+				#interrupt-cells = <2>;
+			};
+
+			gpio1: gpio1@1fbad000 {
+				compatible = "csky,gpio-bank-v1";
+				reg = <0x1fbad000 0x1000>;
+
+				gpio-controller;
+				#gpio-cells = <2>;
+
+				interrupt-controller;
+				#interrupt-cells = <2>;
+			};
+
+			gpio2: gpio2@1fbae000 {
+				compatible = "csky,gpio-bank-v1";
+				reg = <0x1fbae000 0x1000>;
+
+				gpio-controller;
+				#gpio-cells = <2>;
+
+				interrupt-controller;
+				#interrupt-cells = <2>;
