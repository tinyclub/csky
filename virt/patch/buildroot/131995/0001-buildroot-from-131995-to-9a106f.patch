+choice
+	prompt "C-SKY External toolchain C library"
+	default BR2_TOOLCHAIN_EXTERNAL_CSKY_GLIBC
+
+config BR2_TOOLCHAIN_EXTERNAL_CSKY_UCLIBC
+	bool "uClibc/uClibc-ng"
